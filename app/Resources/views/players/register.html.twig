{% extends 'base.html.twig' %}
{% block body %}

    {% for msg in app.session.flashBag.get('error') %}
        <div class="alert alert-danger">
            {{ msg }}
        </div>
    {% endfor %}

    {{ form_start(playerForm) }}
    {{ form_row(playerForm.nickname) }}
    {{ form_row(playerForm.email) }}
    {{ form_row(playerForm.password.first) }}
    {{ form_row(playerForm.password.second) }}
    {{ form_row(playerForm.birthdate) }}
    <input type="submit" value="Register">
{{ form_end(playerForm) }}

{% endblock %}